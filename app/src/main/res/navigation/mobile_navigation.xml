<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.healvi.speaky.presentation.dashboard.ui.module.ModuleFragment"
        android:label="@string/title_module"
        tools:layout="@layout/fragment_module" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.healvi.speaky.presentation.dashboard.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.healvi.speaky.presentation.dashboard.ui.pratice.PraticeFragment"
        android:label="@string/title_practice"
        tools:layout="@layout/fragment_pratice" />
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.healvi.speaky.presentation.dashboard.ui.history.HistoryFragment"
        android:label="@string/title_history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.healvi.speaky.presentation.dashboard.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>