   "source": [
    "from pathlib import Path\n",
    "from PIL import Image\n",
    "\n",
    "inputPath = Path(\"Opened_r\")\n",
    "inputFiles = inputPath.glob(\"**/*.png\")\n",
    "outputPath = Path(\"Opened_jpg\")\n",
    "for f in inputFiles:\n",
    "    outputFile = outputPath / Path(f.stem + \".jpg\")\n",
    "    im = Image.open(f)\n",
    "    im.save(outputFile)"
   ]
